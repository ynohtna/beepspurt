{
  "throwError": true,
  "extends": "airbnb/base",
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "comma-dangle": [2, "never"],
    "indent": [2, 2, {"SwitchCase": 1}],
    "no-console": 1,
	"no-unused-vars": 1,
    "arrow-body-style": 1,
    "jsx-quotes": [1, "prefer-single"]
  }
}
