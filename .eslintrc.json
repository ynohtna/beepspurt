{
  "throwError": true,
  "extends": "airbnb/base",
  "env": {
    "node": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "comma-dangle": [2, "never"],
    "indent": [2, 2, {"SwitchCase": 1}],
    "no-console": 1,
	"no-unused-vars": 1
  }
}
